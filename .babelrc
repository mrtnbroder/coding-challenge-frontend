{
  "presets": ["react", "es2015"],
  "plugins": ["transform-runtime"],
  "env": {
    "webpack": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ],
    }
  }
}
